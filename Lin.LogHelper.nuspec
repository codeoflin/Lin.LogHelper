<?xml version="1.0"?>
<package>
  <metadata>
    <id>Lin.LogHelper</id>
    <version>0.0.1</version>
    <title>Lin.LogHelper</title>
    <authors>Lin</authors>
    <owners>Lin</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <projectUrl>https://github.com/codeoflin/Lin.LogHelper</projectUrl>
    <!--iconUrl></iconUrl -->
